{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build For Linux",
            "command": "/usr/bin/g++",
            "args": [
                "-g",
                "-std=c++20",
                "${workspaceFolder}/src/Main.cpp",
                "${workspaceFolder}/src/engine/input/mouse/MouseInput.cpp",
                "${workspaceFolder}/src/engine/input/key/KeyInput.cpp",
                "${workspaceFolder}/src/files/Files.cpp",
                "${workspaceFolder}/src/engine/math/JMath.cpp",
                "${workspaceFolder}/src/engine/math/vector/v2/Vector2i.cpp",
				"${workspaceFolder}/src/engine/math/vector/v2/Vector2f.cpp",
				"${workspaceFolder}/src/engine/math/vector/v2/Vector2d.cpp",
                "${workspaceFolder}/src/engine/math/vector/v3/Vector3i.cpp",
                "${workspaceFolder}/src/engine/math/vector/v3/Vector3f.cpp",
                "${workspaceFolder}/src/engine/math/vector/v3/Vector3d.cpp",
				"${workspaceFolder}/src/engine/math/vector/quaternion/Quaternion.cpp",
				"${workspaceFolder}/src/engine/math/matrix/mat3/Matrix3d.cpp",
                "${workspaceFolder}/src/engine/parser/json/JsonFile.cpp",
                "${workspaceFolder}/src/engine/parser/json/JsonReader.cpp",
                "${workspaceFolder}/src/engine/parser/json/JsonWriter.cpp",
                "${workspaceFolder}/src/engine/parser/json/jsonContent/JsonObject.cpp",
                "${workspaceFolder}/src/engine/parser/json/jsonContent/JsonArray.cpp",
                "${workspaceFolder}/src/engine/parser/json/jsonContent/JsonMap.cpp",
                "${workspaceFolder}/src/engine/audio/Audio.cpp",
                "${workspaceFolder}/src/engine/audio/AudioFile.cpp",
                "${workspaceFolder}/src/engine/graphics/shader/Shader.cpp",
                "${workspaceFolder}/src/engine/graphics/shader/cubeShader/CubeShader.cpp",
                "-o",
                "${workspaceFolder}/out/linux/Main",
                "-lGLEW",
                "-lGL",
                "-lglfw3",
                "-lX11",
                "-lrt",
                "-lpthread",
                "-lm",
                "-ldl",
                "-lopenal",
                "-lalut"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },


        {
            "type": "cppbuild",
            "label": "C/C++: g++ build For Window",
            "command": "x86_64-w64-mingw32-g++",
            "args": [
                "-g",
                "-std=c++17",
                "-static-libgcc",
                "-static-libstdc++",
                "${workspaceFolder}/src/Main.cpp",
                "${workspaceFolder}/src/engine/input/mouse/MouseInput.cpp",
                "${workspaceFolder}/src/engine/input/key/KeyInput.cpp",
                "${workspaceFolder}/src/files/Files.cpp",
                "${workspaceFolder}/src/engine/math/JMath.cpp",
                "${workspaceFolder}/src/engine/math/vector/v2/Vector2i.cpp",
				"${workspaceFolder}/src/engine/math/vector/v2/Vector2f.cpp",
				"${workspaceFolder}/src/engine/math/vector/v2/Vector2d.cpp",
                "${workspaceFolder}/src/engine/math/vector/v3/Vector3i.cpp",
                "${workspaceFolder}/src/engine/math/vector/v3/Vector3f.cpp",
                "${workspaceFolder}/src/engine/math/vector/v3/Vector3d.cpp",
				"${workspaceFolder}/src/engine/math/vector/quaternion/Quaternion.cpp",
				"${workspaceFolder}/src/engine/math/matrix/mat3/Matrix3d.cpp",
                "${workspaceFolder}/src/engine/parser/json/JsonFile.cpp",
                "${workspaceFolder}/src/engine/parser/json/JsonReader.cpp",
                "${workspaceFolder}/src/engine/parser/json/JsonWriter.cpp",
                "${workspaceFolder}/src/engine/parser/json/jsonContent/JsonObject.cpp",
                "${workspaceFolder}/src/engine/parser/json/jsonContent/JsonArray.cpp",
                "${workspaceFolder}/src/engine/parser/json/jsonContent/JsonMap.cpp",
                "${workspaceFolder}/src/engine/audio/Audio.cpp",
                "${workspaceFolder}/src/engine/audio/AudioFile.cpp",
                "${workspaceFolder}/src/engine/graphics/shader/Shader.cpp",
                "-o",
                "${workspaceFolder}/out/window/Main",
                "-lglfw3dll",
                "-lglew32",
                "-lopengl32",
                "-lgdi32",
                "-lpthread",
                "-lwinpthread",
                "${workspaceFolder}/out/window/OpenAL32.dll"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
    ],
    "version": "2.0.0"
}